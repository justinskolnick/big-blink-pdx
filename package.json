  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "esbuild app/ui/index.tsx --bundle --jsx=automatic --format=esm --target=es2021 --sourcemap --outfile=app/public/javascripts/index.js",
    "build-production": "esbuild app/ui/index.tsx --bundle --jsx=automatic --format=esm --target=es2021 --sourcemap --minify --outfile=app/public/javascripts/index.min.js",
    "watch": "esbuild app/ui/index.tsx --bundle --jsx=automatic --format=esm --target=es2021 --sourcemap --outfile=app/public/javascripts/index.js --watch",
    "start": "nodemon DEBUG=app:* node ./app/bin/www",
    "lint": "eslint app --ext .tsx,.ts,.js --cache --cache-location ./tmp/.eslintcache",
    "test": "jest"
  },
  "test": "jest",
